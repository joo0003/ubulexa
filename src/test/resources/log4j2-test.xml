<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">

  <Appenders>
    <Console name="console" target="SYSTEM_OUT">
      <PatternLayout pattern="%d{ISO8601} %-5p (%t) [%c(%M:%L)] %m%n"/>
    </Console>
  </Appenders>

  <Loggers>
    <Root level="error" additivity="false">
      <AppenderRef ref="console"/>
    </Root>
  </Loggers>

</Configuration>